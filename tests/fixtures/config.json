{
  "agents": {
    "defaults": {
      "workspace": "~/.nanobot/workspace",
      "model": "anthropic/claude-opus-4-5",
      "maxTokens": 8192,
      "temperature": 0.7,
      "maxToolIterations": 20,
      "memoryWindow": 50
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "token": "test-bot-token-123",
      "allowFrom": ["user1", "user2"]
    },
    "slack": {
      "enabled": false,
      "mode": "socket",
      "botToken": "",
      "appToken": ""
    },
    "discord": {
      "enabled": false,
      "token": ""
    }
  },
  "providers": {
    "anthropic": {
      "apiKey": "sk-ant-test-key"
    },
    "openrouter": {
      "apiKey": "sk-or-test-key",
      "apiBase": "https://openrouter.ai/api/v1"
    },
    "deepseek": {
      "apiKey": ""
    }
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18790
  },
  "tools": {
    "web": {
      "search": {
        "apiKey": "",
        "maxResults": 5
      }
    },
    "exec": {
      "timeout": 60
    },
    "restrictToWorkspace": false,
    "mcpServers": {
      "test-server": {
        "command": "npx",
        "args": ["-y", "test-mcp-server"],
        "env": {}
      }
    }
  }
}
