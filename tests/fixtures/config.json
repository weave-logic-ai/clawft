{
  "agents": {
    "defaults": {
      "workspace": "~/.nanobot/workspace",
      "model": "deepseek/deepseek-chat",
      "maxTokens": 8192,
      "temperature": 0.7,
      "maxToolIterations": 20,
      "memoryWindow": 50
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "token": "test-bot-token-123",
      "allowFrom": ["user1", "user2"]
    },
    "slack": {
      "enabled": false,
      "mode": "socket",
      "botToken": "",
      "appToken": ""
    },
    "discord": {
      "enabled": false,
      "token": ""
    }
  },
  "providers": {
    "anthropic": {
      "apiKey": "sk-ant-test-key",
      "browserDirect": true
    },
    "openrouter": {
      "apiKey": "sk-or-test-key",
      "apiBase": "https://openrouter.ai/api/v1"
    },
    "deepseek": {
      "apiKey": ""
    }
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18790,
    "apiPort": 18789,
    "corsOrigins": ["http://localhost:5173"],
    "apiEnabled": false
  },
  "tools": {
    "web": {
      "search": {
        "apiKey": "",
        "maxResults": 5
      }
    },
    "exec": {
      "timeout": 60
    },
    "restrictToWorkspace": false,
    "mcpServers": {
      "test-server": {
        "command": "npx",
        "args": ["-y", "test-mcp-server"],
        "env": {},
        "internalOnly": false
      }
    }
  },
  "voice": {
    "enabled": false,
    "audio": {
      "sampleRate": 16000,
      "chunkSize": 512,
      "channels": 1
    },
    "vad": {
      "threshold": 0.5,
      "silenceTimeoutMs": 1500
    }
  }
}
